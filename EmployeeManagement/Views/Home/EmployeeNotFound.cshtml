@model int
@{
    ViewBag.Title = "404 Error";
}

<div class="alert alert-danger text-center mt-1 mb-1">
    <h4>404 Not Found Error : </h4>
    <hr />
    <h5> Employee with Id = @Model cannot be found </h5>
</div>
<div class="text-center mb-1">
    <a asp-action="index" asp-controller="home" class="btn btn-outline-success float-none">
        Click here to see the list of all employees
    </a>
</div>
