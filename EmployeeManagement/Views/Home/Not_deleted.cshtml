@model Employee

@{
    ViewData["Title"] = "Not_deleted";
       
    }

<div class="row justify-content-center m-3">
    <div class="col-sm-8">
        <div class="card">
            <div class="card-header alert alert-danger text-center" style="margin-bottom:0">
                <h1> <span class="h1 text-dark">@Model.Name</span> is not Delete !! The employee was not found</h1>
                <h3>An error occurred while deleting the employee :@Model.Name</h3>
            </div>
            
            <div class="card-footer text-center">
                <a asp-controller="Home" asp-action="index"
                   class="btn btn-outline-primary col-lg-4 mr-2">Back to List</a>
            </div>
        </div>
    </div>
</div>

